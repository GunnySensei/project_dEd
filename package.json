{
  "name": "project_ded",
  "version": "1.0.0",
  "description": "React website with MongoDB and GraphQL",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && yarn watch\" \"cd client && yarn start\"",
