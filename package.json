{
  "name": "project_ded",
  "version": "1.0.0",
  "description": "React website with MongoDB and GraphQL",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && yarn watch\" \"cd client && yarn start\"",
    "seed": "cd server && yarn seed",
    "install": "cd server && yarn install && cd ../client && yarn install",
    "build": "cd client && yarn run build"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "graphql": "^16.3.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GunnySensei/project_dEd.git"
